    "customwidgets": {
        "previsioni-meteo-ita": {
            "template": "<div ng-init=\"ServerPath='../static/weather-data'; IconSet='flat_colorful'\">\n<link rel=\"stylesheet\" type=\"text/css\" href=\"{{ServerPath}}/layouts/example.css\" />\n</div>\n\n<div ng-init=\"daysnames={'Sunday': 'Domenica', 'Monday': 'Lunedì', 'Tuesday': 'Martedì', 'Wednesday' : 'Mercoledì', 'Thursday' : 'Giovedì', 'Friday' : 'Venerdì', 'Saturday' : 'Sabato'}\">\n<div ng-init=\"statesnames={'Sereno': 'sunny', 'Parzialmente nuvoloso': 'partly-cloudy-day', 'Possibilità di pioggia': 'rain-and-sleet'}\">\n</div>\n\n<table id=\"weather-table\">\n\t<tr>\n\t\t<td colspan=\"9\" id=\"header\" align=\"center\" ng-if=\"itemValue('observationTime')!='UNDEF'\"><b>Ora di osservazione {{itemValue('observationTime') | date:'d/M/yy H:mm'}}</b></td>\n\t\t<td colspan=\"9\" id=\"header\" align=\"center\" ng-if=\"itemValue('observationTime')=='UNDEF'\"><b>Ora di osservazione --/--/----</b></td>\n\t</tr>\n\t<tr colspan=\"9\">\n\t <td height=\"10\"></td>\n\t</tr>\n\t<tr>\n\t  <td rowspan=\"2\" colspan=\"3\" ng-if=\"itemValue('currentCondition')!='UNDEF'\"><img id=\"weather-icon\" src=\"{{itemValue('iconCondition')}}\"/></td>\n    <td rowspan=\"2\" colspan=\"3\" ng-if=\"itemValue('currentCondition')=='UNDEF'\"><img id=\"weather-icon\" src=\"{{ServerPath}}/images/{{IconSet}}/3200.png\"/></td>\n\t\t<td colspan=\"3\" id=\"weather-temp\"  align=\"center\" ng-if=\"itemValue('temperature')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('temperature')}}<font id=\"weather-temp-sign\">°C</font></td>\n\t\t<td colspan=\"3\" id=\"weather-temp\"  align=\"center\" ng-if=\"itemValue('temperature')=='UNDEF'\">--</td>\n\t</tr>\n\t<tr>\n\t\t<td>\n\t\t\t<table id=\"weather-table-details\">\n\t\t\t\t<tr>\n\t\t\t\t\t<td align=\"left\">Umidità relativa:</td>\n\t\t\t\t</tr>\n\t\t\t\t<tr>\n\t\t\t\t\t<td align=\"left\">Pressione:</td>\n\t\t\t\t</tr>\n\t\t\t</table>\n\t\t</td>\n\t\t<td>\n\t\t\t<table id=\"weather-table-details\">\n\t\t\t\t<tr>\n\t\t\t\t\t<td align=\"right\" ng-if=\"itemValue('relativeHumidity')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('relativeHumidity')}} %</td>\n\t\t\t\t\t<td align=\"right\" ng-if=\"itemValue('relativeHumidity')=='UNDEF'\">-- %</td>\n\t\t\t\t</tr>\n\t\t\t\t<tr>\n\t\t\t\t\t<td align=\"right\" ng-if=\"itemValue('pressure')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('pressure')}} hPa</td>\n\t\t\t\t\t<td align=\"right\" ng-if=\"itemValue('pressure')=='UNDEF'\">-- hPa</td>\n\t\t\t\t</tr>\n\t\t\t</table>\n\t\t</td>\n\t</tr>\n\t<tr colspan=\"9\">\n\t <td height=\"20\"></td>\n\t</tr>\n\t<tr>\n\t\t<td/>\n\t\t<td>Oggi</td>\n    <td>Domani</td>\n\t\t<td ng-if=\"itemValue('forecastTime2')!='UNDEF'\">{{daysnames[(itemValue('forecastTimeTomorrow') | date:'EEEE')]}}</td>\n\t\t<td ng-if=\"itemValue('forecastTime2')=='UNDEF'\">--</td>\n\t\t<td ng-if=\"itemValue('forecastTime3')!='UNDEF'\">{{daysnames[(itemValue('forecastTime2') | date:'EEEE')]}}</td>\n\t\t<td ng-if=\"itemValue('forecastTime3')=='UNDEF'\">--</td>\n\t\t<td ng-if=\"itemValue('forecastTime4')!='UNDEF'\">{{daysnames[(itemValue('forecastTime4') | date:'EEEE')]}}</td>\n\t\t<td ng-if=\"itemValue('forecastTime4')=='UNDEF'\">--</td>\n\t\t<td ng-if=\"itemValue('forecastTime5')!='UNDEF'\">{{daysnames[(itemValue('forecastTime5') | date:'EEEE')]}}</td>\n\t\t<td ng-if=\"itemValue('forecastTime5')=='UNDEF'\">--</td>\n\t\t<td ng-if=\"itemValue('forecastTime6')!='UNDEF'\">{{daysnames[(itemValue('forecastTime6') | date:'EEEE')]}}</td>\n\t\t<td ng-if=\"itemValue('forecastTime6')=='UNDEF'\">--</td>\n\t\t<td ng-if=\"itemValue('forecastTime7')!='UNDEF'\">{{daysnames[(itemValue('forecastTime7') | date:'EEEE')]}}</td>\n\t\t<td ng-if=\"itemValue('forecastTime7')=='UNDEF'\">--</td>\n\t</tr>\n\t<tr>\n\t\t<td/>\n\t\t<td ng-if=\"itemValue('forecastTodayCondition')!='UNDEF'\">\n\t\t\t<img id=\"weather-icon\" src=\"{{itemValue('iconforecastTodayCondition')}}\"/>\n\t\t</td>\n\t\t<td ng-if=\"itemValue('forecastTodayCondition')=='UNDEF'\">\n\t\t\t<img id=\"weather-icon\" src=\"{{ServerPath}}/images/{{IconS