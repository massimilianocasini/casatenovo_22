            "template": "<div ng-init=\"ServerPath='../static/weather-data'\">\n  <link rel=\"stylesheet\" type=\"text/css\" href=\"{{ServerPath}}/layouts/animated.css\" /></div> \n<div ng-init=\"daysnames={'Sunday': 'Domenica', 'Monday': 'Lunedì', 'Tuesday': 'Martedì', 'Wednesday' : 'Mercoledì', 'Thursday' : 'Giovedì', 'Friday' : 'Venerdì', 'Saturday' : 'Sabato'}\"></div>\n\n<table id=\"weather-table\">\n\t<tr>\n  \t<td ng-if=\"itemValue('observationTime')!='UNDEF'\"><b>Ora di osservazione {{itemValue('observationTime') | date:'d/M/yy H:mm'}}</b></td>\n\t\t<td ng-if=\"itemValue('observationTime')=='UNDEF'\"><b>Ora di osservazione --/--/----</b></td>\n\t</tr>\n\n\t<tr>\n\t  <td  ng-if=\"itemValue('currentCondition')!='UNDEF'\">\n      <img style=\"filter: invert(60%) sepia(100%) hue-rotate(185deg) saturate(1000%);\" src=\"/icon/climacon?iconset=climacons&format=svg&state={{itemValue('currentCondition')}}\" /></td>\n    <td  ng-if=\"itemValue('currentCondition')=='UNDEF'\">\n      <img id=\"weather-icon\" src=\"{{ServerPath}}/images/{{IconSet}}/3200.png\"/></td>\n      \n\t\t<td colspan=\"3\"  ng-if=\"itemValue('currentTemperature')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('currentTemperature')}}<font id=\"weather-temp-sign\">°C</font></td>\n\t\t<td colspan=\"3\"  ng-if=\"itemValue('currentTemperature')=='UNDEF'\">--</td>\n  </tr>\n    </table>\n\t<tr>\n\t\t<td>\n\t\t\t<table id=\"weather-table-details\">\n\t\t\t\t<tr>\n\t\t\t\t\t<td align=\"left\">Umidità relativa:</td>\n\t\t\t\t</tr>\n\t\t\t\t<tr>\n\t\t\t\t\t<td align=\"left\">Pressione:</td>\n\t\t\t\t</tr>\n\t\t\t</table>\n\t\t</td>\n    \n\t\t<td>\n\t\t\t<table id=\"weather-table-details\">\n\t\t\t\t<tr>\n\t\t\t\t\t<td align=\"center\" ng-if=\"itemValue('currentRelativeHumidity')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('currentRelativeHumidity')}} %</td>\n\t\t\t\t\t<td align=\"center\" ng-if=\"itemValue('currentRelativeHumidity')=='UNDEF'\">-- %</td>\n\t\t\t\t</tr>\n\t\t\t\t<tr>\n\t\t\t\t\t<td align=\"center\" ng-if=\"itemValue('currentPressure')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('currentPressure')}} hPa</td>\n\t\t\t\t\t<td align=\"center\" ng-if=\"itemValue('currentPressure')=='UNDEF'\">-- hPa</td>\n\t\t\t\t</tr>\n    \t</table>\t\t\t\n\t\t</td>\n\t</tr>\n\t<tr colspan=\"7\"><td height=\"20\"></td></tr>\n    \n<table id=\"weather-forecast-table\">\n  \n  <tr>\n  \t<td/>\n\t  <td>Oggi</td>\n    <td>Domani</td>\n\t\t<td ng-if=\"itemValue('forecastTime2')!='UNDEF'\">{{daysnames[(itemValue('forecastTime2') | date:'EEEE')]}}</td>\n\t\t<td ng-if=\"itemValue('forecastTime2')=='UNDEF'\">--</td>\n\t\t<td ng-if=\"itemValue('forecastTime3')!='UNDEF'\">{{daysnames[(itemValue('forecastTime3') | date:'EEEE')]}}</td>\n\t\t<td ng-if=\"itemValue('forecastTime3')=='UNDEF'\">--</td>\n\t\t<td ng-if=\"itemValue('forecastTime4')!='UNDEF'\">{{daysnames[(itemValue('forecastTime4') | date:'EEEE')]}}</td>\n\t\t<td ng-if=\"itemValue('forecastTime4')=='UNDEF'\">--</td>\n\t</tr>\n\n  <tr>\n    \n\t\t<td/>\n\t\t<td ng-if=\"itemValue('forecastTodayCondition')!='UNDEF'\">\n      <img style=\"filter: invert(60%) sepia(100%) hue-rotate(185deg) saturate(1000%);\" src=\"/icon/climacon?iconset=climacons&format=svg&state={{itemValue('forecastTodayCondition')}}\" /></td>\n\t\t\t<td ng-if=\"itemValue('forecastTodayCondition')=='UNDEF'\">\n\t\t\t<img id=\"weather-icon\" src=\"{{ServerPath}}/images/{{IconSet}}/3200.png\"/></td>\n\t    \n\t\t<td ng-if=\"itemValue('forecastTomorrowCondition')!='UNDEF'\">\t\n      <img style=\"filter: invert(60%) sepia(100%) hue-rotate(185deg) saturate(1000%);\" src=\"/icon/climacon?iconset=climacons&format=svg&state={{itemValue('forecastTomorrowCondition')}}\" /></td>\n\t\t<td ng-if=\"itemValue('forecastTomorrowCondition')=='UNDEF'\">\n\t\t\t<img id=\"weather-icon\" src=\"{{ServerPath}}/images/{{IconSet}}/3200.png\"/></td>\n\t\t\n    <td ng-if=\"itemValue('forecast2Condition')!='UNDEF'\">\n       <img style=\"filter: invert(60%) sepia(100%) hue-rotate(185deg) saturate(1000%);\" src=\"/icon/climacon?iconset=climacons&format=svg&state={{itemValue('forecast2Condition')}}\" /></td>\n\t\t<td ng-if=\"itemValue('forecast2Condition')=='UNDEF'\">\n\t\t\t<img id=\"weather-icon\" src=\"{{ServerPath}}/images/{{IconSet}}/3200.png\"/></td>\n\t\t\n    <td ng-if=\"itemValue('forecast3Condition')!='UNDEF'\">\n\t\t\t<img style=\"filter: invert(60%) sepia(100%) hue-rotate(185deg) saturate(1000%);\" src=\"/icon/climacon?iconset=climacons&format=svg&state={{itemValue('forecast3Condition')}}\" /></td>\n\t\t<td ng-if=\"itemValue('forecast3Condition')=='UNDEF'\">\n\t\t\t<img id=\"weather-icon\" src=\"{{ServerPath}}/images/{{IconSet}}/3200.png\"/></td>\n\t\t\n    <td ng-if=\"itemValue('forecast4Condition')!='UNDEF'\">\n\t\t\t<img style=\"filter: invert(60%) sepia(100%) hue-rotate(185deg) saturate(1000%);\" src=\"/icon/climacon?iconset=climacons&format=svg&state={{itemValue('forecast4Condition')}}\" /></td>\n\t\t<td ng-if=\"itemValue('forecast4Condition')=='UNDEF'\">\n\t\t\t<img id=\"weather-icon\" src=\"{{ServerPath}}/images/{{IconSet}}/3200.png\"/></td>\t\n\t\n      </tr>\n\t\n  <tr>\n\t\t<td class=\"temp-max\" >Max</td>\n\t\t<td class=\"temp-max\" ng-if=\"itemValue('forecastTodayMaxTemperature')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastTodayMaxTemperature')}} °C</td>\n\t\t<td class=\"temp-max\" ng-if=\"itemValue('forecastTodayMaxemperature')=='UNDEF'\">-- °C</td>\n\t\t<td class=\"temp-max\" ng-if=\"itemValue('forecastTomorrowMaxTemperature')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastTomorrowMaxTemperature')}} °C</td>\n\t\t<td class=\"temp-max\" ng-if=\"itemValue('forecastTomorrowMaxTemperature')=='UNDEF'\">-- °C</td>\n\t\t<td class=\"temp-max\" ng-if=\"itemValue('forecastDay2MaxTemperature')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastDay2MaxTemperature')}} °C</td>\n\t\t<td class=\"temp-max\" ng-if=\"itemValue('forecastDay2MaxTemperature')=='UNDEF'\">-- °C</td>\n\t\t<td class=\"temp-max\" ng-if=\"itemValue('forecastDay3MaxTemperature')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastDay3MaxTemperature')}} °C</td>\n\t\t<td class=\"temp-max\" ng-if=\"itemValue('forecastDay3MaxTemperature')=='UNDEF'\">-- °C</td>\n\t\t<td class=\"temp-max\" ng-if=\"itemValue('forecastDay4MaxTemperature')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastDay4MaxTemperature')}} °C</td>\n\t\t<td class=\"temp-max\" ng-if=\"itemValue('forecastDay4MaxTemperature')=='UNDEF'\">-- °C</td>\n\t</tr>\n  \n\t<tr>\n\t\t<td class=\"temp-min\" >Min</td>\n\t\t<td class=\"temp-min\" ng-if=\"itemValue('forecastTodayMinTemperature')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastTodayMinTemperature')}} °C</td>\n\t\t<td class=\"temp-min\" ng-if=\"itemValue('forecastTodayMinTemperature')=='UNDEF'\">-- °C</td>\n\t\t<td class=\"temp-min\" ng-if=\"itemValue('forecastTomorrowMinTemperature')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastTomorrowMinTemperature')}} °C</td>\n\t\t<td class=\"temp-min\" ng-if=\"itemValue('forecastTomorrowMinTemperature')=='UNDEF'\">-- °C</td>\n\t\t<td class=\"temp-min\" ng-if=\"itemValue('forecastDay2MinTemperature')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastDay2MinTemperature')}} °C</td>\n\t\t<td class=\"temp-min\" ng-if=\"itemValue('forecastDay2MinTemperature')=='UNDEF'\">-- °C</td>\n\t\t<td class=\"temp-min\" ng-if=\"itemValue('forecastDay3MinTemperature')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastDay3MinTemperature')}} °C</td>\n\t\t<td class=\"temp-min\" ng-if=\"itemValue('forecastDay3MinTemperature')=='UNDEF'\">-- °C</td>\n\t\t<td class=\"temp-min\" ng-if=\"itemValue('forecastDay4MinTemperature')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastDay4MinTemperature')}} °C</td>\n\t\t<td class=\"temp-min\" ng-if=\"itemValue('forecastDay4MinTemperature')=='UNDEF'\">-- °C</td>\t\n\t</tr>\n  \n\t<tr>\n\t\t<td class=\"col-xs-4\">Piovosità</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('TodayProbaPrecipitation')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('TodayProbaPrecipitation')}} %</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('TodayProbaPrecipitation')=='UNDEF'\">-- %</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastTomorrowProbaPrecipitation')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastTomorrowProbaPrecipitation')}} %</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastTomorrowProbaPrecipitation')=='UNDEF'\">-- %</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastDay2ProbaPrecipitation')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastDay2ProbaPrecipitation')}} %</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastDay2ProbaPrecipitation')=='UNDEF'\">-- %</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastDay3ProbaPrecipitation')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastDay3ProbaPrecipitation')}} %</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastDay3ProbaPrecipitation')=='UNDEF'\">-- %</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastDay4ProbaPrecipitation')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastDay4ProbaPrecipitation')}} %</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastDay4ProbaPrecipitation')=='UNDEF'\">-- %</td>\n\t</tr>\n  \n\t<tr>\n\t\t<td class=\"col-xs-4\"></td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('TodayPrecipitation')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('TodayPrecipitation')}} mm</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('TodayPrecipitation')=='UNDEF'\">-- mm</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastTomorrowPrecipitation')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastTomorrowPrecipitation')}} mm</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastTomorrowPrecipitation')=='UNDEF'\">-- mm</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastDay2Precipitation')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastDay2Precipitation')}} mm</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastDay2Precipitation')=='UNDEF'\">-- mm</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastDay3Precipitation')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastDay3Precipitation')}} mm</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastDay3Precipitation')=='UNDEF'\">-- mm</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastDay4Precipitation')!='UNDEF'\">{{'%.0f' | sprintf:itemValue('forecastDay4Precipitation')}} mm</td>\n\t\t<td class=\"col-xs-4\" ng-if=\"itemValue('forecastDay4Precipitation')=='UNDEF'\">-- mm</td>\n\t</tr>\n</table>\t"
        }
